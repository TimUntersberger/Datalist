  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TimUntersberger/Datalyst/issues"
  },
  "homepage": "https://github.com/TimUntersberger/Datalyst#readme",
  "devDependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/mysql": "^2.15.7",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "electron": "^6.0.10",
    "nodemon": "^1.19.2",
    "parcel": "^1.12.3",
    "tsc-watch": "^4.0.0",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "app-root-path": "^2.2.1",
    "electron-is-dev": "^1.1.0",
    "mysql": "^2.17.1",
